{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx":"react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@public/*": ["src/public/*"],
      "@src/*": ["src/*"],
      "@layouts/*": ["src/layouts/*"],
    }
  }
}
